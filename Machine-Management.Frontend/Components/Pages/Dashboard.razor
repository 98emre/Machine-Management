@page "/dashboard"

<PageTitle>Dashboard</PageTitle>

<h3>Dashboard</h3>

<div class="mt-2">
    <button class="btn btn-primary">New Device</button>
</div>

<div class="container mt-3">
    <div class="row">
        <div class="col-md-3">
           <h3>Extra information</h3>
        </div>
        
        <div class="col-md-4">
            <h4>List of devices</h4>
            <table class="table table-striped table-bordered table-hover mt-3">
                    <thead>
                        <tr>
                            <th>#</th>
                            <th>DeviceId</th>
                            <th>Name</th>
                            <th>Date</th>
                            <th>Status</th>
                        </tr>
                    </thead>
        
                    <tbody>
                            <!-- Lägg till dynamiskt genererat tabellinnehåll här -->
                 
                    </tbody>
             </table>
        </div>
    </div>
</div>



